{
  "name": "rcarvello/webmvcframework",
  "description": "PHP Web MVC Framework",
  "type": "framework",
  "license": "MIT",
  "homepage": "https://github.com/rcarvello/webmvcframework",
  "authors": [
    {
      "name": "Rosario Carvello",
      "email": "rosario.carvello@gmail.com"
    }
  ],
  "config": {
        "allow-plugins": true
   },
  "require": {
    "composer/installers": "~1.0",
    "php": ">=5.3",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-mysqli": "*",
    "ext-ctype": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-gd": "*"
  },
  "minimum-stability": "master",
  "extra": {
    "installer-paths": {
      ".": ["rcarvello/webmvcframework"]
    }
  },
 "autoload": {
    "psr-4": {
      "\\": "/"
    }
  }
}
